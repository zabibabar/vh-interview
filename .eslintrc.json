{
  "extends": ["airbnb", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "parser": "flow",
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "warn"
  },
  "env": {
    "browser": true
  }
}
